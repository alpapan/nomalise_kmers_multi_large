{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "norm kmer test multiple files",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [
        "--forward",
        "a1",
        "a2",
        "--reverse",
        "b1",
        "b2",
        "--depth",
        "2",
        "--verbose",
        "--debug",
        "1",
        "-p",
        "2",
        "-m",
        "1000001"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "gcc build active file",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "norm kmer test1",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [
        "--forward",
        "forward.fastq",
        "--reverse",
        "reverse.fastq",
        "--depth",
        "2",
        "--verbose",
        "--debug",
        "1",
        "-p",
        "2",
        "-m",
        "1000001"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "gcc build active file",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}
